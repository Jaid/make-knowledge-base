{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tsx test/test.ts",
      "autoAttachChildProcesses": true,
      "console": "internalConsole",
      "env": {
        "DEBUG": "${workspaceFolderBasename},${workspaceFolderBasename}:*",
        "DEBUG_COLORS": "1",
        "DEBUG_HIDE_DATE": "1",
        "FORCE_COLOR": "3",
        "NODE_ENV": "development"
      },
      "internalConsoleOptions": "neverOpen",
      "request": "launch",
      "runtimeExecutable": "${env:NVM_SYMLINK}/node.exe",
      "type": "node",
      "runtimeArgs": [
        "--import",
        "tsx",
        "--",
        "test/test.ts"
      ]
    }
  ],
  "compounds": []
}
