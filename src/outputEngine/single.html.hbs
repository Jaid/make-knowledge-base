<!DOCTYPE html>
<html lang=en>
  <body>
    {{#each pages}}
      <div>
        <h1>Knowledge Base: {{@key}}</h1>
        {{#each this}}
          {{#if title}}
            <h2>{{title}}</h2>
          {{else if entry.title}}
            <h2>{{entry.title}}</h2>
          {{/if}}
          {{renderContentModule this}}
        {{/each}}
      </div>
    {{/each}}
  </body>
</html>
